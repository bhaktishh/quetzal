func isPrime (Nat n) of Bool {
    let Nat i = 2;
    while (i < n) {
        if (n) {
i = i + 1;         } else {
i = i + 2;         }
        i = i + 1; 
    }
    switch (n) {
        case (0) {
            i = i + 6; 
        }
        case (2) {
            i = i + 6; 
        }
    }
    return True;
}