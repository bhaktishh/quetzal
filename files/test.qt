func binSearch (Nat key, Vect(n, Nat) v) of Fin(n) {
    let Fin(n) low = 0;
    let Fin(n) high = n - 1; 
    let Fin(n) mid = 0;
    let Nat x = 0;
    let Fin(n) ret = 0;
    while (low < (high + 1)) {
        mid = (low + high) / 2;
        x = index(mid, v); 
        if (key == x) {
            ret = mid;
        } else {
            if (key < x) {
                high = mid - 1;
            } else {
                low = mid + 1; 
            }
        }
    }
    return ret; 
}