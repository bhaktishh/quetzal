import Data.Vect
import Data.Nat
import Decidable.Equality

func search (Nat n, Vect(n, Nat) ls, Nat x) of Maybe(Fin(n)) {
    let Nat i = 0;
    let Maybe(Fin(n)) ret = Nothing; 
    while(i < n) {
        if ((index(natToFinLT(i), ls)) == x) {
            ret = Just((natToFinLT(i))); 
        }
        else {;;}
        i = 1 + i; 
    }
    return ret; 
}